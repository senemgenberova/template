<!DOCTYPE HTML>

<html lang="az" class="no-js">

<head>
    <meta http-equiv="content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title>@ViewBag.Title</title>
    <meta name="description" content="">
    <meta name="author" content="">

    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon" />
    <link rel="stylesheet" href="~/Public/bootstrap/css/bootstrap.css">
    @RenderSection("herflerCss", false)
    @RenderSection("adlarCss", false)
    @RenderSection("tehsilCss", false)
    @RenderSection("nameCss", false)
    @RenderSection("cizgiCss", false)
    @RenderSection("foodblogCss", false)
    @RenderSection("idmanCss", false)
    @RenderSection("foodCss", false)
    @RenderSection("healthCss", false)
    @RenderSection("homeCSs", false)
    @RenderSection("dr_profileCss", false)
    @RenderSection("complexCss", false)
    @RenderSection("allComplexCss", false)

    <link href="~/Public/style.css" rel="stylesheet" type="text/css">
    <link href="~/Public/css/shortcodes.css" rel="stylesheet" type="text/css">
    <link href="~/Public/css/responsive.css" rel="stylesheet" type="text/css">
    <link href="~/Public/css/font-awesome.min.css" rel="stylesheet" type="text/css">

 
    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700,300italic,400italic,700italic' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800'
          rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Bubblegum+Sans' rel='stylesheet' type='text/css'>
    <!--jquery-->
   
    <script src="~/Public/js/modernizr-2.6.2.min.js"></script>
</head>

<body class="main">
    <!--wrapper starts-->
    <div class="wrapper">
        <!--header starts-->
        <header>
            <div class="container">
                <div class="logo">
                    <a href="index.html" title="Kids Life">
                        <img src="~/Public/images/logo2.png" alt="kids life" title="kids life" style="width:150px;">
             
                    </a>
                </div>
                <div class="login-details">
                    <p class="login">
                        <a href="register.html">Qeydiyyat <i class="fa fa-user login_icon" aria-hidden="true"></i></a>
                        <a href="login.html">Giriş <i class="fa fa-sign-in login_icon" aria-hidden="true"></i></a>
                    </p>
                </div>
            </div>
            <!--menu-container starts-->
            <div id="menu-container">
                <div class="container">
                    <!--nav starts-->
                    <nav id="main-menu">
                        <div class="dt-menu-toggle" id="dt-menu-toggle">Menu<span class="dt-menu-toggle-icon"></span></div>
                        <ul id="menu-main-menu" class="menu">
                            <li class="current_page_item  red">
                                @Html.ActionLink("Ana Səhifə", "Index", "Home")
                            </li>
                            <li class="mustard">
                                @Html.ActionLink("Adlar", "Index", "Letters")
                            </li>
                            <li class="yellow">
                               @Html.ActionLink(" Cizgi Filmlər", "Index", "Cartoon")
                            </li>
                            <li class="blue">
                               @Html.ActionLink("Idman", "Index", "gym")
                            </li>
                            <li class="steelblue">
                                @Html.ActionLink("Qida", "Index", "Food")
                            </li>
                            <li class="lavender">
                                @Html.ActionLink("Təhsil", "Index", "Education")
                            </li>
                            <li class="purple">
                                @Html.ActionLink("Sağlamlıq", "Index", "Health")
                            </li>
                        </ul>
                    </nav>
                    <!--nav ends-->

                    <ul id="icons" class="dt-sc-social-icons">
                        <li>
                            <a href="#" class="dt-sc-tooltip-top search">
                                <span class="fa fa-search"></span>
                            </a>
                        </li>
                        <li>
                            <a href="#" title="Facebook" class="dt-sc-tooltip-top facebook">
                                <span class="fa fa-facebook"></span>
                            </a>
                        </li>
                        <li>
                            <a href="#" title="Twitter" class="dt-sc-tooltip-top twitter">
                                <span class="fa fa-twitter"></span>
                            </a>
                        </li>
                        <li>
                            <a href="#" title="Google Plus" class="dt-sc-tooltip-top gplus">
                                <span class="fa fa-google-plus"></span>
                            </a>
                        </li>
                        <div class="search_tool">
                            <input type="text" placeholder="Axtar">
                        </div>
                    </ul>
                </div>
            </div>
            <!--menu-container ends-->
        </header>
        <!--header ends-->
        @RenderBody()
        <!--footer starts-->
        <footer>
            <!--footer-widgets-wrapper starts-->
            <div class="footer-widgets-wrapper">
                <!--container starts-->
                <div class="container">
                    <div id="main">
                        <aside class="widget widget_text">
                            <h3 class="dt-sc-hr-green-title">Kontakt</h3>
                            <div class="row contact">
                                <div class="col-md-4 col-sm-4 col-xs-12">
                                    <div class="contact_info">
                                        <i class="fa fa-map-marker" aria-hidden="true"></i>
                                        <p>Nizami küç. 203B, AF Business House, 2-ci mərtəbə</p>
                                    </div>
                                </div>
                                <div class="col-md-4 col-sm-4 col-xs-12">
                                    <div class="contact_info">
                                        <i class="fa fa-envelope" aria-hidden="true"></i>
                                        <p>Code.edu.az</p>
                                    </div>
                                </div>
                                <div class="col-md-4 col-sm-4 col-xs-12">
                                    <div class="contact_info">
                                        <i class="fa fa-phone" aria-hidden="true"></i>
                                        <p>+994 70 333 27 77</p>
                                    </div>
                                </div>
                            </div>
                        </aside>
                    </div>
                </div>
                <!--container ends-->
            </div>
            <!--footer-widgets-wrapper ends-->
            <div class="copyright">
                <div class="container">
                    <p class="copyright-info">© 2014 Kids Life . All rights reserved.</p>
                    <div class="footer-links">
                        <p>Follow us</p>
                        <ul class="dt-sc-social-icons">
                            <li class="facebook">
                                <a href="#"><img src="~/Public/images/facebook.png" alt="" title=""></a>
                            </li>
                            <li class="twitter">
                                <a href="#"><img src="~/Public/images/twitter.png" alt="" title=""></a>
                            </li>
                            <li class="gplus">
                                <a href="#"><img src="~/Public/images/gplus.png" alt="" title=""></a>
                            </li>
                            <li class="pinterest">
                                <a href="#"><img src="~/Public/images/pinterest.png" alt="" title=""></a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </footer>
        <!--footer ends-->
    </div>
    <!--wrapper ends-->
    <a href="#" title="Go to Top" class="back-to-top">To Top ↑</a>

    @RenderSection("tehsilJS",false)
    @RenderSection("foodblogJs",false)
    @RenderSection("homeJs", false)
    @RenderSection("carouselJS", false)

